function [c] = ZFEqualizer(h,L,overSampling)
%ZFEQUALIZER Summary of this function goes here
%   Detailed explanation goes here
k = 1;
for i=1:ceil(length(h)/overSampling)
    h_coeff(i) = h((i-1)*overSampling+1);
end
N = length(h_coeff);
hM = toeplitz([h_coeff([N-1:end]) zeros(1,L-lengt(h_coeff([N-1:end])))], [ h_coeff([N-1:-1:1]) zeros(1,(N-1)*k+1-L+1) ]);
e  = zeros(1,(N-1)*k+1);
e(k+1) = 1;
c  = [pinv(hM)*e.'].';

end

